{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug STM32 (OpenOCD)",
      "type": "cortex-debug",
      "request": "launch",

      "servertype": "openocd",
      "cwd": "${workspaceFolder}",
      "executable": "${workspaceFolder}/blink.elf",
      "device": "STM32F401RE",
      "configFiles": [
        "interface/stlink.cfg",
        "target/stm32f4x.cfg"
      ],
      "runToEntryPoint": "main",

      "svdFile": "${workspaceFolder}/STM32F401.svd",
      "showDevDebugOutput": "parsed"
    }
  ]
}
